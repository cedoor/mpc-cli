{
    "name": "@mpc-cli/create",
    "type": "module",
    "version": "0.2.3",
    "description": "A streamlined tool for creating Multi-Party Computation (MPC) projects with ease.",
    "license": "MIT",
    "bin": {
        "mpc-cli-create": "dist/index.js"
    },
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": "https://github.com/cedoor/mpc-cli",
    "homepage": "https://github.com/cedoor/mpc-cli/tree/main/packages/cli",
    "bugs": {
        "url": "https://github.com/cedoor/mpc-cli.git/issues"
    },
    "engines": {
        "node": ">=14.16"
    },
    "scripts": {
        "start": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./src/index.ts",
        "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin typescript",
        "prepublishOnly": "yarn build"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/figlet": "^1.5.8",
        "@types/inquirer": "^9.0.7",
        "@types/pacote": "^11.1.8",
        "@types/semver": "^7.5.8",
        "rimraf": "^5.0.5",
        "rollup": "^4.12.0",
        "rollup-plugin-cleanup": "^3.2.1"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "boxen": "^7.1.1",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "figlet": "^1.7.0",
        "inquirer": "^9.2.15",
        "log-symbols": "^6.0.0",
        "ora": "^8.0.1",
        "pacote": "^17.0.6",
        "semver": "^7.6.0"
    }
}
